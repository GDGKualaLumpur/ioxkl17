{
  "entrypoint": "index.html",
  "shell": "src/io-app.html",
  "fragments": [
    "src/io-home.html",
    "src/io-sessions.html",
    "src/io-attending.html",
    "src/io-cod.html"
  ],
  "sources": [
    "src/**/*",
    "data/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "_redirects",
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "name": "appspot",
    "bundle": false,
    "js": {"minify": true, "compile": true},
    "css": {"minify": true},
    "html": {"minify": true},
    "addPushManifest": true,
    "insertPrefetchLinks": false,
    "addServiceWorker": true
  },{
    "name": "firebase",
    "bundle": false,
    "js": {"minify": true, "compile": true},
    "css": {"minify": true},
    "html": {"minify": true},
    "addPushManifest": true,
    "insertPrefetchLinks": true,
    "addServiceWorker": true
  }]
}
